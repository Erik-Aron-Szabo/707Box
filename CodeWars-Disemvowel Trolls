def disemvowel(string):
    string2 = [] #if space -----> (convert it to) NO SPACE
    for i in range(len(string)):
        if string[i] == "a" or string[i] == "o" or string[i] == "e" or string[i] == "i" or string[i] == "u" or string[i] == "A" or string[i] == "I" or string[i] == "O" or string[i] == "U" or string[i] == "E": 
            i += 1
        else:
            string2.append(string[i])
    return ''.join(string2)
    
    
    
    
print(disemvowel("This website is for losers LOL!"))
